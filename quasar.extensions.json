{
  "@quasar/dotenv": {
    "env_development": ".env",
    "env_production": ".env",
    "common_root_object": "none",
    "create_env_files": false,
    "add_env_to_gitignore": false
  },
  "@quasar/icon-genie": {
    "minify_dev": "pngquant",
    "minify_build": "zopfli",
    "cordova": {
      "background_color": "#000074",
      "splashscreen_type": "pure"
    },
    "build_always": true,
    "__internal": {
      "dev": {
        "electron": {
          "iconHash": "4fbaa6a6bad94bf20dd3ee578482e8ad"
        },
        "spa": {
          "iconHash": "4fbaa6a6bad94bf20dd3ee578482e8ad"
        }
      },
      "build": {
        "electron": {
          "iconHash": "4fbaa6a6bad94bf20dd3ee578482e8ad"
        }
      }
    }
  },
  "@quasar/testing": {
    "harnesses": [
      "unit-jest"
    ]
  },
  "@quasar/testing-unit-jest": {
    "babel": "babelrc",
    "options": [
      "scripts"
    ]
  }
}